<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context="com.automatica.axc_lib.Activities.cIncidencia.Incidencia">


    <include
        android:id="@+id/include2"
        layout="@layout/cl_header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>




        <TextView
            android:id="@+id/txtv_Seleccionado2"
            style="@style/EstiloTexto_Negro_Bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="8dp"
            android:text="Cantidad total"
            app:layout_constraintStart_toEndOf="@+id/txtv_Estatus"
            app:layout_constraintTop_toBottomOf="@id/include2" />

        <TextView
            android:id="@+id/txtv_Estatus"
            style="@style/EstiloTexto_Negro_Reg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="8dp"
            android:text="-"
            android:textStyle="bold|italic"
            app:layout_constraintStart_toEndOf="@+id/txtv_Fecha2"
            app:layout_constraintTop_toBottomOf="@id/include2" />

        <TextView
            android:id="@+id/txtv_Fecha2"
            style="@style/EstiloTexto_Negro_Reg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="8dp"
            android:text="Estatus"
            android:textStyle="bold|italic"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/include2" />

        <TextView
            android:id="@+id/txtv_CantTot"
            style="@style/EstiloTexto_Negro_Reg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="8dp"
            android:text="-"
            app:layout_constraintStart_toEndOf="@+id/txtv_Seleccionado2"
            app:layout_constraintTop_toBottomOf="@id/include2" />

    <TextView
        android:id="@+id/txtv_TipoDetalle"
        style="@style/EstiloTexto_Negro_Bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:text="Partidas"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtv_Fecha2" />

    <View
        android:id="@+id/view4"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:background="@color/colorPrimary"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtv_TipoDetalle">

    </View>


    <ListView
        android:id="@+id/lv_EmpaquesPallet"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintStart_toStartOf="parent"
        >

    </ListView>

</androidx.constraintlayout.widget.ConstraintLayout>
