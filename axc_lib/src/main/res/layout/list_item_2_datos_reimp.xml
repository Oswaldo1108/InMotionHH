<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:descendantFocusability="blocksDescendants">

    <Button
        android:id="@+id/btn_ReimprimirEmpaque"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:text="Imprimir"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_Titulo"
        style="@style/EstiloTexto_Negro_Bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="8dp"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:text="Emp."
        app:layout_constraintBottom_toBottomOf="@+id/txtv_CodigoEmpaque"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/txtv_CodigoEmpaque"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tv_Titulo8"
        style="@style/EstiloTexto_Negro_Bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:text="Producto"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_Titulo" />

    <TextView
        android:id="@+id/tv_Titulo9"
        style="@style/EstiloTexto_Negro_Bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:text="Lote"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_Titulo8" />

    <TextView
        android:id="@+id/tv_Titulo10"
        style="@style/EstiloTexto_Negro_Bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="8dp"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:text="Cant."
        app:layout_constraintBottom_toBottomOf="@+id/tv_Titulo9"
        app:layout_constraintStart_toEndOf="@+id/txtv_Lote"
        app:layout_constraintTop_toTopOf="@+id/tv_Titulo9"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/txtv_CodigoEmpaque"
        style="@style/EstiloTexto_Negro_Reg"
        android:layout_width="wrap_content"

        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:text="-"
        app:layout_constraintBottom_toBottomOf="@+id/btn_ReimprimirEmpaque"
        app:layout_constraintStart_toEndOf="@+id/tv_Titulo"
        app:layout_constraintTop_toTopOf="@+id/btn_ReimprimirEmpaque" />

    <TextView
        android:id="@+id/txtv_Producto"
        style="@style/EstiloTexto_Negro_Reg"
        android:layout_width="wrap_content"

        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:text="-"
        app:layout_constraintBottom_toBottomOf="@+id/tv_Titulo8"
        app:layout_constraintStart_toEndOf="@+id/tv_Titulo8"
        app:layout_constraintTop_toTopOf="@+id/tv_Titulo8" />

    <TextView
        android:id="@+id/txtv_Cantidad"
        style="@style/EstiloTexto_Negro_Reg"
        android:layout_width="wrap_content"

        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:text="-"
        app:layout_constraintBottom_toBottomOf="@+id/tv_Titulo10"
        app:layout_constraintStart_toEndOf="@+id/tv_Titulo10"
        app:layout_constraintTop_toTopOf="@+id/tv_Titulo10"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/txtv_Lote"
        style="@style/EstiloTexto_Negro_Reg"
        android:layout_width="wrap_content"

        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:text="-"
        app:layout_constraintBottom_toBottomOf="@+id/tv_Titulo9"
        app:layout_constraintStart_toEndOf="@+id/tv_Titulo9"
        app:layout_constraintTop_toTopOf="@+id/tv_Titulo9" />

</androidx.constraintlayout.widget.ConstraintLayout>